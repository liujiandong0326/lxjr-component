{
  "version": "1.0.5",
  "name": "lxjr-component",
  "description": "基于 antd vue 的组件库",
  "main": "dist/index.umd.js",
  "keywords": [
    "antd vue",
    "component",
    "table"
  ],
  "author": "mike liujiandong0326@163.com",
  "files": [
    "dist",
    "components"
  ],
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build:js && npm run build:css",
    "build:js": "webpack --config webpack.component.js",
    "build:css": "npx gulp less",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "vue": "^2.6.14"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "ant-design-vue": "^1.7.8",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "gulp": "^4.0.2",
    "gulp-less": "^5.0.0",
    "gulp-minify-css": "^1.2.4",
    "less-loader": "^7.3.0",
    "lodash": "^4.17.21",
    "vue-loader": "^15.9.8",
    "vue-template-compiler": "^2.6.14",
    "vuepress": "^1.9.7",
    "webpack-cli": "^4.9.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "vue/multi-word-component-names": 0
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
